    x:Class="App.App">
    
    <Application.Resources>
        <ResourceDictionary>
            
            <Color x:Key="Dark_MAX">#000000</Color>
            <Color x:Key="Dark_MED">#202020</Color>
            <Color x:Key="Dark_MIN">#404040</Color>
            
            <Color x:Key="Medium_Dark">#606060</Color>
            <Color x:Key="Medium_Medium">#808080</Color>
            <Color x:Key="Medium_Light">#A0A0A0</Color>
            
